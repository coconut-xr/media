{
  "name": "co-media",
  "version": "1.1.0",
  "type": "module",
  "main": "dist/index.js",
  "description": "React library for accessing browser media like camera, microphone, screen capture, etc.",
  "homepage": "https://cocoss-org.github.io/co-media",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Bela Bohlender",
  "keywords": [
    "media",
    "react",
    "camera",
    "screen-share",
    "microphone",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:cocoss-org/co-media.git"
  },
  "peerDependencies": {
    "react": "*"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "check": "run-s check:prettier check:eslint",
    "check:prettier": "prettier --check src/**/*.{ts,tsx}",
    "check:eslint": "eslint src",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:prettier": "prettier --write src/**/*.{ts,tsx}",
    "fix:eslint": "eslint src --fix"
  },
  "dependencies": {
    "suspend-react": "0.0.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "@types/react": "^18.0.26",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
